 "html": "<meta name=\"pdfkit-orientation\" content=\"Landscape\" />\n<div id=\"header-html\" class=\"hidden-pdf\">\n    {% set letter_head = frappe.get_doc(\"Letter Head\",\"Skill Matrix Test\") %}\n    {% if letter_head %}\n    {{ letter_head.content }}\n\n    {% endif %}\n</div>\n<hr>\n<table class=\"table table-bordered row-item\" border=\"1\">\n    <tr>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">Prepared Date</td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\"> </td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">Valid Upto</td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\"></td>\n    </tr>\n    <tr>\n        <th style=\"border:1px solid !important;\" rowspan=\"2\">Legend</th>\n        <th style=\"border:1px solid !important;\" colspan=\"2\">\n            <center>Level-I</center>\n        </th>\n        <th style=\"border:1px solid !important;\" colspan=\"2\">\n            <center>Level-II</center>\n        </th>\n        <th style=\"border:1px solid !important;\" colspan=\"2\">\n            <center>Level-III<center>\n        </th>\n        <th style=\"border:1px solid !important;\" colspan=\"2\">\n            <center>Level-IV<center>\n        </th>\n    </tr>\n    <tr>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">\n            <h6 style=\"text-align:center;\">Can Work Under Continuous Supervision</h6>\n            <center>\n                <table border=\"1\">\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;\"></td>\n                    </tr>\n                    <tr>\n                        <td style=\"border:1px solid !important;\"></td>\n                        <td style=\"border:1px solid !important;\"></td>\n                    </tr>\n                </table>\n            </center>\n        </td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">\n            <h6 style=\"text-align:center;\">Can Work Under Supervision</h6>\n            <center>\n                <table border=\"1\">\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                    </tr>\n                    <tr>\n                        <td style=\"border:1px solid !important;\"></td>\n                        <td style=\"border:1px solid !important;\"></td>\n                    </tr>\n                </table>\n            </center>\n        </td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">\n            <h6 style=\"text-align:center;\">Can Work Independently</h6>\n            <center>\n                <table border=\"1\">\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                    </tr>\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;\"></td>\n                    </tr>\n                </table>\n            </center>\n        </td>\n        <td style=\"border:1px solid !important;\" colspan=\"2\">\n            <h6 style=\"text-align:center;\">Can Train Others</h6>\n            <center>\n                <table border=\"1\">\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                    </tr>\n                    <tr>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                    </tr>\n                </table>\n            </center>\n        </td>\n    </tr>\n</table>\n\n<style type=\"text/css\">\n    @media print {\n\n        /* avoid cutting tr's in half */\n        th div,\n        td div {\n            margin-top: -8px;\n            padding-top: 8px;\n            page-break-inside: avoid;\n        }\n    }\n</style>\n\n<body>\n    <table class=\"table table-bordered row-item\">\n        <thead>\n            <tr>\n                <th rowspan=\"2\" style=\"border:1px solid !important;\">\n                    <center>S.No</center>\n                </th>\n                <th rowspan=\"2\" style=\"border:1px solid !important;\">\n                    <center>User ID</center>\n                </th>\n                <th rowspan=\"2\" style=\"border:1px solid !important;\">\n                    <center>Associate Name</center>\n                </th>\n                <th colspan=\"2\" style=\"border:1px solid !important;\">\n                    <center>Process</center>\n                </th>\n                <th rowspan=\"2\" style=\"border:1px solid !important;\">Skill Level</th>\n            </tr>\n            <tr>\n                <th style=\"border:1px solid !important;\">Gamma</th>\n                <th style=\"border:1px solid !important;\">Kodera</th>\n            </tr>\n        </thead>\n        {% set test_list = frappe.get_all(\"Auto Cutting and Crimping\", filters={'question_set':doc.question_set,'status':'Active' },fields=['employee_code','associate','legend','gamma','kodera'],order_by='date_of_joining asc') %}\n        {%- for row in test_list  -%}\n        <tr>\n            <td align=\"center\" style=\"border:1px solid !important;\"> {{ loop.index }}</td>\n            <td align=\"center\" style=\"border:1px solid !important;\">{{ row.employee_code}}</td>\n            <td align=\"center\" style=\"border:1px solid !important;\">{{ row.associate}}</td>\n            {% if row.gamma%}\n            <td style=\"border:1px solid !important;\" align=\"center\"\n                style=\"font-family: wingdings; font-size:180%;font-weight:bold; font-color:green\">&#10004;</td>\n            {%else%}\n            <td style=\"border:1px solid !important;\"></td>\n            {%endif%}\n            {% if row.kodera%}\n            <td style=\"border:1px solid !important;\" align=\"center\"\n                style=\"font-family: wingdings; font-size:180%;font-weight:bold; font-color:green\">&#10004;</td>\n            {%else%}\n            <td style=\"border:1px solid !important;\"></td>\n            {%endif%}\n            {% if row.legend == 'L1-Trainee' %}\n            <td style=\"border:1px solid !important;\">\n                <center>\n                    <table border=\"1\">\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;\"></td>\n                        </tr>\n                        <tr>\n                            <td style=\"border:1px solid !important;\"></td>\n                            <td style=\"border:1px solid !important;\"></td>\n                        </tr>\n                    </table>\n                </center>\n            </td>\n            {% elif row.legend == 'L2-Can Work under Supervision' %}\n            <td style=\"border:1px solid !important;\">\n                <center>\n                    <table border=\"1\">\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        </tr>\n                        <tr>\n                            <td style=\"border:1px solid !important;\"></td>\n                            <td style=\"border:1px solid !important;\"></td>\n                        </tr>\n                    </table>\n                </center>\n            </td>\n            {% elif row.legend == 'L3-Can Work Independently' %}\n            <td style=\"border:1px solid !important;\">\n                <center>\n                    <table border=\"1\">\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        </tr>\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;\"></td>\n                        </tr>\n                    </table>\n                </center>\n            </td>\n            {% elif row.legend == 'L4-Can Train Others' %}\n            <td style=\"border:1px solid !important;\">\n                <center>\n                    <table border=\"1\">\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        </tr>\n                        <tr>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                            <td style=\"border:1px solid !important;background-color:#22b14c !important\"></td>\n                        </tr>\n                    </table>\n                </center>\n            </td>\n            {% else %}\n            <td style=\"border:1px solid !important;\"></td>\n            {% endif %}\n        </tr>\n        {%- endfor -%}\n    </table>\n</body>\n<div id=\"footer-html\" class=\"visible-pdf\">\n    {% set letter_head = frappe.get_doc(\"Letter Head\",\"Skill Matrix Test\") %}\n    {% if letter_head %}\n    {{ letter_head.footer }}\n    {% endif %}\n    <p class=\"text-center small page-number visible-pdf\">\n        {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n    </p>\n</div>\n\n<!-- </div>\n<div id=\"footer-html\" class=\"visible-pdf\" style=\"display: block !important; position: absolute;\tbottom: 0.75in;\">\n    <div class=\"letter-head-footer\">\n        <div style=\"text-align: center;\">{{ footer }}</div>\n\n        <div id=\"header-html\" class=\"hidden-pdf\">\n            {{ letter_head.footer }}\n        </div> -->\n<!-- <div class=\"row\">\n<p class=\"text-center small page-number visible-pdf\">\n    {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n</p>\n</div> -->\n<!-- </div> -->\n<!-- </div> -->", 
